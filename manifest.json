{
  "manifest_version": 3,
  "name": "Maxa Experiences",
  "short_name": "Maxa Experiences",
  "version": "0.0.1",
  "start_url": ".",
  "description": "Themes created with love for WebXR developers :-)",
  "display": "standalone",
  "icons": [ {
   "src": "./icons/icon512.png",
   "sizes": "512x512",
   "type": "image/png",
   "purpose": "any maskable monochrome"
  },
  {
    "src": "./icons/icon16.png",
    "sizes": "200x200",
    "type": "image/png"
   }],
  "background_color": "#6dcdb1",
  "theme_color": "#009578",
  "chrome_url_overrides": {
      "newtab": "./scenes/index.html"
   },
  "browser_action": {
   "default_icon": "./icons/icon16.png",
   "default_popup": "popup.html"

  },
  "options_page": "options.html",
  
  "permissions" : [
    "storage",
    "notifications", 
    "contextMenus",
     "tabs"
  ],
  "host_permissions": [
  "https://www.Google.com/*"
  ],
  "content_scripts": [ 
    {
      "matches": ["https://dev.coolstart.com/*"],
      "js": ["content.js", "jquery-3.6.0.min.js"]
    }
  ]

   


}